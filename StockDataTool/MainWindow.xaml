<Window x:Class="StockDataTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockDataTool"
        mc:Ignorable="d"
        Title="Stock Tool" Height="456" Width="715" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Closed="Window_Closed">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="407" Margin="10,10,0,0" VerticalAlignment="Top" Width="689">
            <ProgressBar x:Name="progressBar" Height="41" Margin="10,10,10,0"/>
            <TextBox x:Name="logTextBox" TextWrapping="Wrap" Margin="10,10,10,0" Height="302"/>
            <Button x:Name="goButton" Content="Close" Margin="10,10,10,0" Click="goButton_Click"/>
        </StackPanel>

    </Grid>
</Window>
